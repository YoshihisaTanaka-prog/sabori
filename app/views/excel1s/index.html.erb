<p id="notice"><%= notice %></p>

<h1>Excel1s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @excel1s.each do |excel1| %>
      <tr>
        <td><%= excel1.title %></td>
        <td><%= excel1.body %></td>
        <td><%= link_to 'Show', excel1 %></td>
        <td><%= link_to 'Edit', edit_excel1_path(excel1) %></td>
        <td><%= link_to 'Destroy', excel1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Excel1', new_excel1_path %>
