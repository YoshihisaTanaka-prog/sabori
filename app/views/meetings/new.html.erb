<h1>New Meeting</h1>

<%= form_for(@meeting, url: meetings_path, method: :post) do |f| %>
    <%= f.hidden_field :eventId, value: @num %>
    <%= f.label :fiscalYear, "開催年度" %>
    <%= f.number_field :fiscalYear, value: 2020 %><br>
    <%= f.label :eventNumber, "イベント番号" %>
    <%= f.text_field :eventNumber, value: "" %><br>
    <%= f.label :field, "分野" %>
    <%= f.text_field :field, value: "" %><br>
    <%= f.label :title, "タイトル" %>
    <%= f.text_field :title, value: "" %><br>
    <%= f.label :keyword, "キーワード" %>
    <%= f.text_field :keyword, value: "" %><br>
    <%= f.label :organization, "主催" %>
    <%= f.text_field :organization, value: "" %><br>
    <%= f.label :start, "開始日" %>
    <%= f.date_field :start %><br>
    <%= f.label :end, "終了日" %>
    <%= f.date_field :end %><br>
    <%= f.label :place, "場所" %>
    <%= f.text_field :place %><br>
    <%= f.label :url, "URL" %>
    <%= f.text_field :url %><br>
    <%= f.submit "追加" %>
<% end %>

<%= link_to 'Back', meetings_path %>
