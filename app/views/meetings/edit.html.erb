<h1>Editing Meeting</h1>

<%= form_for(@meeting, url: meeting_path, method: :patch) do |f| %>
    <%= f.hidden_field :eventId, value: @meeting.eventId %>
    <%= f.label :fiscalYear, "開催年度" %>
    <%= f.number_field :fiscalYear, value: @meeting.fiscalYear %><br>
    <%= f.label :eventNumber, "イベント番号" %>
    <%= f.text_field :eventNumber, value: @meeting.eventNumber %><br>
    <%= f.label :field, "分野" %>
    <%= f.text_field :field, value: "" %><br>
    <%= f.label :title, "タイトル" %>
    <%= f.text_field :title, value: @meeting.title %><br>
    <%= f.label :keyword, "キーワード" %>
    <%= f.text_field :keyword, value: "" %><br>
    <%= f.label :organization, "主催" %>
    <%= f.text_field :organization, value: @meeting.organization %><br>
    <%= f.label :start, "開始日" %>
    <%= f.date_field :start, value: @meeting.start %><br>
    <%= f.label :end, "終了日" %>
    <%= f.date_field :end, value: @meeting.end %><br>
    <%= f.submit "追加" %>
<% end %>

<%= link_to 'Show', @meeting %> |
<%= link_to 'Back', meetings_path %>
